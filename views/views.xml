<odoo>
  <data>
  
	<record model="ir.ui.view" id="view_id">
		<field name="name">view.name</field>
		<field name="model">object_name</field>
		<field name="priority" eval="16">
		<field name="arch" type="xml">
			<tree string="Alpha list">
				<field name="name"/>
				<field name="date"/>
				<field name="address"/>
				<field name="key"/>
				<field name="betas"/>
			</tree>
			<tree string="Beta list">
				<field name="name"/>
				<field name="phone"/>
				<field name="price"/>
			</tree>
		</field>
	</record>
  </data>
</odoo>